	keywords = {Ambulatory Care Facilities, electronic clinical quality measure (eCQM), Electronic Health Records, Health Care, Health Information Exchange, health information interoperability, Humans, Information Dissemination, Patient Safety, Quality Indicators},
	pages = {1534--1542},
