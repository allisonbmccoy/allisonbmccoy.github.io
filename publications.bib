	keywords = {Algorithms, Benchmarking, Electronic Health Records, Fuzzy Logic, Humans, Medical Record Linkage, Medical Record Linkage [N04.452.859.564.550], Medical Records Systems, Computerized [L01.700.508.300.695], Probability},
	pages = {97--104},
