	keywords = {clinical decision support, comparative effectiveness, complex intervention, Decision Support Systems, Clinical, electronic health record, Randomized Controlled Trials as Topic, reporting guideline},
	pages = {1744--1756},
	file = {Full Text:C\:\\Users\\mccoyab\\Zotero\\storage\\DUXN2W86\\Reese et al. - 2022 - Conceptualizing clinical decision support as compl.pdf:application/pdf},
}

